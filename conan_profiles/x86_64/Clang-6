include(../BuildProfile)

cc_compiler=clang-6.0
cxx_compiler=clang++-6.0

[settings]
    os=Linux
    os_build=Linux
    arch=x86_64
    arch_build=x86_64
    compiler=clang
    compiler.version=6.0
    compiler.libcxx=libstdc++11
    build_type=Release

[env]
    CXX=/usr/bin/clang++-6.0
    CC=/usr/bin/clang-6.0

[options]

